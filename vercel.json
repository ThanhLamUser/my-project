{
  "redirects": [
    {
      "source": "/success/:path*", 
      "destination": "/success",  
      "permanent": true
    },
    {
      "source": "/:path*",        
      "destination": "/",         
      "has": [
        {
          "type": "query",
          "key": "cancel"
        }
      ],
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/success",
      "destination": "/success"
    },
    {
      "source": "/",
      "destination": "/"
    }
  ]
}
